<Page x:Class="VoterFileAnalyzer.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VoterFileAnalyzer"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Voter File Analyzer - MainPage">

    <Grid>
        <StackPanel Background="White">
            <TextBlock Text="Florida Voter File Analyzer" FontSize="18"></TextBlock>
            <Button Name="Import" Content="Import Data" Click="Import_Click" Width="100" Height="30" Margin="0,0,0,10" HorizontalAlignment="Left"></Button>
            <Button Name="Export" Content="Export to Excel" Click="Export_Click" Width="100" Height="30" Margin="0,0,0,10" HorizontalAlignment="Left"></Button>
            
        </StackPanel>
       
    </Grid>
</Page>
